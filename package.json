{
  "name": "react-next-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:schema": "apollo service:download --endpoint=https://rails-base-graphql-api.herokuapp.com/graphql graphql-schema.json",
    "generate:TSFiles": "apollo client:codegen --localSchemaFile=graphql-schema.json --target typescript --includes 'graphql/**/*.graphql' --excludes **/__generated__",
    "gen:types": "yarn generate:schema && yarn generate:TSFiles",
    "build": "tsc --project server/tsconfig.json && next build",
    "server": "ts-node  server/server.ts",
    "start": "NODE_ENV=production NODE_PATH=\".dist/\" node .dist/server/server.js -p $PORT",
    "test": "jest --watch",
    "lint": "yarn lint:js && yarn lint:css:in:js && yarn lint:css:in:ts && yarn lint:types && yarn lint:fix",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:css:in:js": "stylelint './**/*.js'",
    "lint:css:in:ts": "stylelint './**/*.tsx'",
    "lint:css": "yarn lint:css:in:js && yarn lint:css:in:ts",
    "lint:types:ss": "tsc --project server/tsconfig.json --noEmit --pretty",
    "lint:types:cs": "tsc --project tsconfig.json --noEmit --pretty",
    "lint:types": "yarn run lint:types:ss && yarn run lint:types:cs",
    "lint:fix": "yarn lint:js --fix",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "ci:jest": "jest",
    "ci:e2e": "yarn build && start-server-and-test 'yarn start' ${PORT:-3000} 'yarn e2e'",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "print-env": "print-env",
    "heroku-prebuild": "yarn install --production",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.18.1 <=16.11"
  },
  "resolutions": {
    "ts-invariant": "^0.9.4"
  },
  "dependencies": {
    "@apollo/client": "3.3.*",
    "@babel/types": "^7.8.7",
    "body-parser": "^1.19.0",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-force-https": "^1.0.0",
    "formik": "^2.1.4",
    "http-proxy-middleware": "^2.0.1",
    "https-proxy-agent": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest-extended": "^0.11.5",
    "lodash": "^4.17.15",
    "next": "10.2.3",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
    "next-svgr": "^0.0.2",
    "next-with-apollo": "^5.0.0",
    "polished": "^3.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^6.0.8",
    "styled-components": "^5.0.1",
    "typescript": "^4.4.4",
    "universal-cookie": "^4.0.3",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@bahmutov/print-env": "^2.1.2",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/cypress-dotenv": "^1.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "apollo": "^2.33.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cypress": "^6.3.0",
    "cypress-dotenv": "^1.2.2",
    "cypress-file-upload": "^5.0.2",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.2.1-alpha-6bce0355c-20211031",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.0",
    "jest-transform-graphql": "^2.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.1",
    "start-server-and-test": "^1.11.7",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.3.0"
  }
}
