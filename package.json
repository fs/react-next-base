{
  "name": "react-next-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "heroku-prebuild": "yarn install --production",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx pages components",
    "lint:css:in:js": "stylelint './**/*.js'",
    "lint:css:in:ts": "stylelint './**/*.tsx'",
    "lint:css": "yarn lint:css:in:js && yarn lint:css:in:ts",
    "test": "jest --watch",
    "test:ci": "jest",
    "scan": "yarn lint && yarn lint:css && yarn test:ci",
    "cy": "cypress",
    "cy:run": "cypress run",
    "cy:chr": "cypress run --browser chrome",
    "cy:open": "cypress open"
  },
  "engines": {
    "node": "13.9.0"
  },
  "dependencies": {
    "@apollo/react-hoc": "^3.1.4",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@babel/types": "^7.8.7",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@types/yup": "^0.26.36",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-force-https": "^1.0.0",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "http-proxy-middleware": "^1.0.4",
    "https-proxy-agent": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "9.3.5",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
    "next-routes": "^1.4.2",
    "next-svgr": "^0.0.2",
    "next-with-apollo": "^5.0.0",
    "polished": "^3.4.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.3",
    "universal-cookie": "^4.0.3",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cypress": "^4.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
