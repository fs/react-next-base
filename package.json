    "https-proxy-agent": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest-extended": "^0.11.5",
    "lodash": "^4.17.15",
    "next": "10.2.3",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
    "next-svgr": "^0.0.2",
